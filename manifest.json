{
  "manifest_version": 2,

  "name": "WebExtension",
  "description": "This little helper keeps track of your web usage and periodically alerts the user of time consumption",
  "version": "1.0",
  
  "background":{
	"scripts":["popup.js"]
  },
  
  "browser_action":{
	"default_icon": "icon.png",
	"default_popup":"popup.html",
	"default_title": "Click Here!"
  },
  
 
  "permissions": [
	"tabs",
    "activeTab",
	"storage",
	"notifications",
    "https://ajax.googleapis.com/"
  ]
}